    "sass-loader": "^7.1.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "webpack-cli": "^3.1.2"
  },
  "rescripts": [
    "env"
  ],
  "proxy": "https://localhost:5001"
}
