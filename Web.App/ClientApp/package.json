  "scripts": {
    "start": "react-scripts start",
    "start:jsonserver": "pushd ..\\JsonServer && npx nodemon --watch . jsonserver.js",
    "start:dotnet-core": "npm run build:dotnet-core && pushd .. && npx cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "start:hypernovacomponentserver": "pushd ..\\HypernovaComponentServer && npx nodemon --watch server-bundle.js componentserver.js",
    "start:server-bundle": "npm run build:server-bundle:development && npx chokidar-cli src/** -c \"npm run build:server-bundle:development\"",
    "start:storybook": "start-storybook -p 9001 -c .storybook -s ./public",
    "build": "react-scripts build",
    "build:dotnet-core": "pushd ..\\.. && dotnet restore && dotnet build && popd",
    "build:server-bundle:development": "cross-env NODE_ENV=development cross-env PUBLIC_URL=$npm_package_appdir cross-env SERVER_BUNDLE_RELATIVE_OUTPUT_FOLDER=../HypernovaComponentServer webpack --config webpack.server-bundle.config.js",
    "build:server-bundle:production": "cross-env NODE_ENV=production cross-env PUBLIC_URL=$npm_package_appdir cross-env SERVER_BUNDLE_RELATIVE_OUTPUT_FOLDER=../HypernovaComponentServer webpack --config webpack.server-bundle.config.js",
    "build:storybook": "build-storybook -c .storybook -o storybookbuild -s ./public",
    "gen:apiclients": "nswag run /runtime:NetCore22 /input:nswag.json",
    "test": "react-scripts test",
    "plop": "plop"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [ 
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react-helmet": "^5.0.8",
