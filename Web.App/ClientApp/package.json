    "raw-loader": "^2.0.0",
    "react-scripts": "^3.0.1",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.4.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "webpack-cli": "^3.3.1"
  },
  "rescripts": [
    "env"
  ],
  "proxy": "https://localhost:5001"
