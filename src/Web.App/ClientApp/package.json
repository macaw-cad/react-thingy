  "scripts": {
    "start": "react-scripts start",
    "start:dotnet": "npm run build:dotnet-core && run-p start:web.app start:web.api.versioned",
    "start:jsonserver": "pushd ..\\JsonServer && npx nodemon --watch . jsonserver.js",
    "start:web.app": "pushd .. && npx cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "start:web.api.versioned": "pushd ..\\..\\Web.Api.Versioned && npx cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "start:hypernovacomponentserver": "pushd ..\\HypernovaComponentServer && npx nodemon --watch server-bundle.js componentserver.js",
    "start:server-bundle": "npm run build:server-bundle:development && npx chokidar-cli src/** -c \"npm run build:server-bundle:development\"",
    "start:storybook": "start-storybook -p 9001 -c .storybook -s ./public",
    "build": "react-scripts build",
    "build:dotnet-core": "pushd ..\\.. && dotnet restore && dotnet build && popd",
    "build:server-bundle:development": "cross-env NODE_ENV=development cross-env PUBLIC_URL=$npm_package_appdir cross-env SERVER_BUNDLE_RELATIVE_OUTPUT_FOLDER=../HypernovaComponentServer/bundles webpack --config webpack.server-bundle.config.js",
    "build:server-bundle:production": "cross-env NODE_ENV=production cross-env PUBLIC_URL=$npm_package_appdir cross-env SERVER_BUNDLE_RELATIVE_OUTPUT_FOLDER=../HypernovaComponentServer/bundles webpack --config webpack.server-bundle.config.js",
    "build:serge": ".\\node_modules\\.bin\\webpack --version",
    "build:storybook": "build-storybook -c .storybook -o storybookbuild -s ./public",
    "gen:apiclients": "npx npm-run-all genapiclients:*",
    "genapiclients:WebAppClients": "nswag run /runtime:NetCore22 /input:nswag.json /variables:Project=../../Web.App/Web.App.csproj,OutputOpenApi=src/api/WebAppOpenApi.json,Output=src/api/WebAppClients.ts",
    "_genapiclients:WebAppRestful": "strip-bom src/api/WebAppOpenApi.json > src/api/WebAppOpenApi.nobom.json && restful-react import --file src/api/WebAppOpenApi.nobom.json --output src/api/WebAppRestful.tsx",
    "genapiclients:WebApiVersionedClients": "nswag run /runtime:NetCore22 /input:nswag.json /variables:Project=../../Web.Api.Versioned/Web.Api.Versioned.csproj,OutputOpenApi=src/api/WebApiVersionedOpenApi.json,Output=src/api/WebApiVersionedClients.ts",
    "_genapiclients:WebApiVersionedRestful": "strip-bom src/api/WebApiVersionedOpenApi.json > src/api/WebApiVersionedOpenApi.nobom.json && restful-react import --file src/api/WebApiVersionedOpenApi.nobom.json --output src/api/WebApiVersionedRestful.tsx",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --env=jsdom",
    "plop": "plop"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@owja/ioc": "^1.0.0",
    "@types/react-helmet": "^5.0.15",
    "node-sass": "^4.13.0",
