    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "webpack-cli": "^3.3.10"
  },
  "rescripts": [
    "env"
  ],
  "proxy": "https://localhost:5001"
